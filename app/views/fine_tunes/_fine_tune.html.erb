<div id="<%= dom_id fine_tune %>">
  <p class="my-5 text-white">
    <strong class="block mb-1 font-medium">Name:</strong>
    <%= fine_tune.name %>
  </p>

  <p class="my-5 text-white">
    <strong class="block mb-1 font-medium">Platform:</strong>
    <%= fine_tune.platform %>
  </p>

  <p class="my-5 text-white">
    <strong class="block mb-1 font-medium">Platform source:</strong>
    <%= fine_tune.platform_source %>
  </p>

  <p class="my-5 text-white">
    <strong class="block mb-1 font-medium">Platform link:</strong>
    <%= fine_tune.platform_link %>
  </p>

  <p class="my-5 text-white">
    <strong class="block mb-1 font-medium">Model:</strong>
    <%= @models.find(fine_tune.model_id).name %>
  </p>

</div>
