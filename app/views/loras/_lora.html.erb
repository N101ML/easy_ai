<div class="flex text-white border-b border-gray-200">
  <div class="p-2 basis-1/5">
    <%= lora.name %>
  </div>

  <div class="p-2 basis-1/5">
    <%= lora.url_src %>
  </div>

  <div class="p-2 basis-1/5">
    <%= lora.trigger %>
  </div>

  <div class="p-2 basis-1/5">
    <%= lora.platform %>
  </div>

  <div class="p-2 basis-1/5">
    <%= link_to model_path(lora.model) do %>
      <%= lora.model.name %>
    <% end %>
  </div>
</div>